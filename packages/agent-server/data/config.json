{
  "agents": [
    {
      "agentId": "claude-cli",
      "displayName": "Claude Code",
      "description": "Claude Code",
      "chat": {
        "provider": "claude-cli",
        "models": [
          "opus",
          "sonnet",
          "haiku"
        ],
        "config": {
          "extraArgs": [
            "--dangerously-skip-permissions"
          ]
        }
      }
    },
    {
      "agentId": "codex-cli",
      "displayName": "Codex",
      "description": "Codex",
      "chat": {
        "provider": "codex-cli",
        "models": [
          "gpt-5.2-codex",
          "gpt-5.2"
        ],
        "thinking": [
          "off",
          "low",
          "medium",
          "high",
          "xhigh"
        ],
        "config": {
          "extraArgs": [
            "--skip-git-repo-check",
            "--dangerously-bypass-approvals-and-sandbox"
          ]
        }
      }
    },
    {
      "agentId": "pi",
      "displayName": "pi",
      "description": "pi",
      "chat": {
        "provider": "pi-cli",
        "models": [
          "anthropic/claude-sonnet-4-5",
          "openai-codex/gpt-5.2-codex"
        ],
        "thinking": [
          "off",
          "low",
          "medium",
          "high",
          "xhigh"
        ]
      }
    }
  ],
  "profiles": [
    { "id": "default", "label": "Default" },
    { "id": "personal", "label": "Personal" },
    { "id": "work", "label": "Work" }
  ],
  "plugins": {
    "chat": {
      "enabled": true
    },
    "panels": {
      "enabled": true
    },
    "sessions": {
      "enabled": true
    },
    "agents": {
      "enabled": true
    },
    "url-fetch": {
      "enabled": true
    },
    "lists": {
      "enabled": true,
      "gitVersioning": {
        "enabled": true,
        "intervalMinutes": 1
      },
      "instances": [
        "personal",
        "work"
      ]
    },
    "notes": {
      "enabled": true,
      "gitVersioning": {
        "enabled": true,
        "intervalMinutes": 1
      },
      "instances": [
        "personal",
        "work"
      ]
    },
    "search": {
      "enabled": true
    },
    "time-tracker": {
      "enabled": true,
      "gitVersioning": {
        "enabled": true,
        "intervalMinutes": 1
      },
      "instances": [
        "personal",
        "work"
      ]
    },
    "files": {
      "enabled": true,
      "workspaceRoot": "~"
    },
    "diff": {
      "enabled": true,
      "gitVersioning": {
        "enabled": true,
        "intervalMinutes": 1
      },
      "instances": [
        {
          "id": "default",
          "label": "Default",
          "workspaceRoot": "${HOME}/worktrees"
        },
        {
          "id": "work",
          "label": "Work",
          "workspaceRoot": "${HOME}/work"
        }
      ]
    },
    "terminal": {
      "enabled": true
    },
    "links": {
      "enabled": true,
      "spotify": {
        "rewriteWebUrlsToUris": true
      }
    }
  }
}
